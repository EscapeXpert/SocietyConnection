<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <title>
        <%= title %>
    </title>
</head>
<body>
<table >
    <thead>
    <tr>
        <th>번호</th>
        <th>제목</th>
        <th>작성자</th>
        <th>작성일</th>
        <th>조회수</th>
    </tr>
    </thead>
    <tbody>
    <% for(let post of posts) { %>
        <tr>
            <td><%= post.id %></td>
            <td><%= post.title %></td>
            <td><%= post.nickname %></td>
            <% const date = new Date(post.created_at) %>
            <td><%= date.toISOString().slice(0, 10) %></td>
            <td><%= post.view_count %></td>
        </tr>
    <% } %>
    </tbody>
</table>
</body>
</html>